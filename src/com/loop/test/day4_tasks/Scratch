 List<WebElement> list = driver.findElements(By.xpath("//span[@class='body-1']"));
        WebElement home = list.get(0);
        WebElement received = list.get(1);
        WebElement invitations = list.get(3);


        System.out.println(home.getText() + home.isDisplayed());
        System.out.println(received.getText() + received.isDisplayed());
        System.out.println(invitations.getText() + invitations.isDisplayed());

        menuButton.click();
        System.out.println(home.getText() + home.isDisplayed());
        System.out.println(received.getText() + received.isDisplayed());
        System.out.println(invitations.getText() + invitations.isDisplayed());

